<h1>Account: <%= @account.name %></h1>
<div class="span6">
	<table class="table table-striped table-bordered">
		<tr><td>Account</td><td><%= @account.name  %></td></tr>
		<tr><td>Subdomain</td><td><%= @account.subdomain  %></td></tr>
		<tr><td>Email</td><td><%= @account.email  %></td></tr>
		<tr><td>Phone #</td><td><%= @account.phone %></td></tr>
		<tr><td>Website</td><td><%= @account.website %></td></tr>
		<tr><td>Address</td><td><%= @account.address %></td></tr>

	</table>
	<%= link_to 'All Accounts', accounts_path %>
</div>

